
@{
	ViewBag.Title = "История оказанных услуг";
}

<h2>История оказанных услуг</h2>

<div id="scheduler"></div>

<div id="reportWindow">
	<form id="reportForm"></form>
</div>

<button id="reportWindowButton">Печать</button>

<script id="schedulerEditor" type="text/x-kendo-template">
	<div class="container">
	<form id="schedulerForm">
		<p>
			<label>Начало: <input id="schedulerDatePicker" name="DateOfCreate" required /></label>
		</p>
		<p>
			<label>Услуга: <input id="schedulerService" data-role="combobox" name="Service.Id" required /></label>
		</p>
		<p>
			<label>Компания: <input id="schedulerCompany" data-role="combobox" name="Company.Id" required /></label>
		</p>
		<p>
			<label>Сотрудник: <input id="schedulerEmployee" data-role="combobox" name="Employee.Id" required /></label>
		</p>
	</form>
	</div>
</script>